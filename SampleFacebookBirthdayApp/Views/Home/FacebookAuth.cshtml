@using Microsoft.AspNet.Facebook
@{
    ViewBag.Title = "Facebook Authentication";
    var AppID = @GlobalFacebookConfiguration.Configuration.AppId;
}
<script src="~/Scripts/common.js"></script>
<script src="//connect.facebook.net/en_US/all.js"></script>
<script>
    window.fbAsyncInit = function () {
        FB.init({
            appId: '@GlobalFacebookConfiguration.Configuration.AppId', // App ID
            status: true, // check login status
            cookie: true, // enable cookies to allow the server to access the session
            xfbml: true  // parse XFBML
        });
        AppWiseProcess();
    };
    function AppWiseProcess() {
        if (getParameterByName("AppName") == "NavratriSpecial") {
            NavratriSpecial();
        }
        else if (getParameterByName("AppName") == "WishNavratri") {
            WishNavratri();
        }

    }
  
</script>
